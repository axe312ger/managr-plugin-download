{
  "name": "managr-plugin-download",
  "version": "1.0.0",
  "description": "Adds download support for single files.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel --out-dir dist src",
    "dev": "babel --watch --out-dir dist src",
    "lint": "eslint src test",
    "test": "npm run lint && npm run coverage-tests",
    "coverage-tests": "NODE_ENV=test nyc --reporter=lcov --reporter=text --check-coverage --lines 100 npm run unit-tests",
    "unit-tests": "ava --verbose",
    "unit-tests-watch": "ava --verbose --watch"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-istanbul": "^2.0.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^4.0.1",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "nyc": "^8.3.0",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.3.1"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "file-saver": "^1.3.2",
    "react": "^15.3.1"
  }
}
